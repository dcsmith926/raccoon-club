    "compilerOptions": {
        "outDir": "./public/",
        "noImplicitAny": true,
        "module": "ES6",
        "moduleResolution": "node",
        "target": "ES2015",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
    },
}